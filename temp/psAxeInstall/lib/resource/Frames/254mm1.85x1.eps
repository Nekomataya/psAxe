/Aspect {AspectH AspectW div} def
/FrameHeight {FrameWidth Aspect mul} def
/CBaseHeight {PaperHeight CBaseOffset sub} def
/PegBaseHeight {PaperHeight PegBarOffset sub} def


% hall drawing
% レジストリ孔を描く
%　直径 1/4 インチ
/HallDraw
{
		PegForm 1 gt
		{
	 newpath
	 0 0 9 0 PaperWidth 2 div arc
	PegForm 3 eq
	{
		 closepath
		 .5 setgray
		 gsave
		 fill
		 grestore
	}
	{
		 0 setgray
		 stroke
	} ifelse
		}
		{
	PegForm 1 eq
	{
	 gsave
	.5 in CrossDraw
	 grestore
	} if
		} ifelse
} def

% slot drawing
% サイドスロットを描く
% 原点位置はスロットの中心
% 幅 5/8 インチ ・ 高さ 1/8 インチ
/SlotDraw
{
		PegForm 1 gt
		{
			newpath

			 -20.25 4.50 moveto
			 22.5 4.5 22.5 -4.5 4.50 arct
			 22.5 -4.5 -22.5 -4.5 4.50 arct
			 -22.5 -4.5 -22.5 4.5 4.50 arct
			 -22.5 4.5 0 4.50 4.50 arct


	PegForm 3 eq
	{
		closepath
		.5 setgray
		gsave
		fill
		grestore
	}
	{
		 0 setgray
		 stroke
	} ifelse
		}
		{
	PegForm 1 eq
	{
	 gsave	1 in
		dup 2 div neg 0 rmoveto
		dup 0 rlineto
%		dup 2 div neg 0 rmoveto
		dup 4 div 0 exch rmoveto
		dup 2 div 0 exch neg rlineto
		dup neg 0 rmoveto
		2 div 0 exch rlineto
		0.02 cm setlinewidth
		.0 setgray
		stroke
	 grestore
	} if
		} ifelse
} def

% mainframe draw

/DrawMainFrame
{ FrameWidth 2 div neg FrameHeight 2 div neg moveto
 FrameWidth 0 cm rlineto
 0 cm FrameHeight rlineto
 FrameWidth neg 0 cm rlineto
closepath
0.05 cm setlinewidth
.0 setgray
% fill
stroke
} def

/DrawDashLineH
{ FrameWidth 2 div neg 0 moveto
 5 mm 0 cm rlineto
 FrameWidth 2 div 0 cm moveto
 5 mm neg 0 cm rlineto
0.015 cm setlinewidth
.0 setgray
%[1 3] 2 setdash
stroke
} def

/DrawDashLineV
{ 0 FrameHeight 2 div neg moveto
 0 5 mm rlineto
 0 FrameHeight 2 div moveto
 0 5 mm neg rlineto
0.015 cm setlinewidth
.0 setgray
%[1 3] 2 setdash
stroke
} def

/DrawSafeFrame
{ SafeWidth 2 div neg SafeWidth Aspect mul 2 div neg moveto
 SafeWidth 0 cm rlineto
 0 cm SafeWidth Aspect mul rlineto
 SafeWidth neg 0 cm rlineto
closepath
0.02 cm setlinewidth
.0 setgray
[2 3] 2 setdash
% fill
stroke
} def

/DrawTitleSafeArea
{
 TitleSafeWidth 2 div neg 0 rmoveto
	gsave
	currentpoint translate
	.1 in CrossDraw
	grestore

 0 TitleSafeWidth Aspect mul 2 div neg rmoveto
	gsave
	currentpoint translate
	.2 in CrossDraw
	grestore

 TitleSafeWidth 2 div 0 cm rmoveto
	gsave
	currentpoint translate
	.1 in CrossDraw
	grestore

 TitleSafeWidth 2 div 0 cm rmoveto
	gsave
	currentpoint translate
	.2 in CrossDraw
	grestore

 0 cm TitleSafeWidth Aspect mul 2 div rmoveto
	gsave
	currentpoint translate
	.1 in CrossDraw
	grestore

 0 cm TitleSafeWidth Aspect mul 2 div rmoveto
	gsave
	currentpoint translate
	.2 in CrossDraw
	grestore

 TitleSafeWidth 2 div neg 0 cm rmoveto
	gsave
	currentpoint translate
	.1 in CrossDraw
	grestore

 TitleSafeWidth 2 div neg 0 cm rmoveto
	gsave
	currentpoint translate
	.2 in CrossDraw
	grestore

%closepath
%0.02 cm setlinewidth
%.0 setgray
%[2 3] 2 setdash
% fill
%stroke
} def

/DrawMarginFrame
{ MarginWidth 2 div neg MarginWidth Aspect mul 2 div neg moveto
 MarginWidth 0 cm rlineto
 0 cm MarginWidth Aspect mul rlineto
 MarginWidth neg 0 cm rlineto
closepath
0.02 cm setlinewidth
.0 setgray
[1 3] 2 setdash
% fill
stroke
} def

% 穴あけ用ガイド
% 原点位置はホールの中心
% 幅 238 mm ・ 高さ 1 インチ
/DrawTappingGuide
{
119 mm neg 18 rmoveto
0 72 neg rlineto
238 mm 0 rmoveto
0 72 rlineto

0.02 cm setlinewidth
.0 setgray

stroke
} def

%	クロス 描画
%		スタックにクロスのサイズを置いてから呼び出す
%		3 cm CrossDraw
/CrossDraw
{
	dup 2 div neg 0 rmoveto
	dup 0 rlineto
	dup 2 div neg 0 rmoveto
	dup 2 div 0 exch rmoveto
	0 exch neg rlineto
		0.02 cm setlinewidth
		.0 setgray
		stroke
} def

