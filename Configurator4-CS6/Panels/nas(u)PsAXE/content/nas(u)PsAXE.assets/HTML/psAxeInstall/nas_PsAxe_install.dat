//app.path + "/"+ localize("$$$/ScriptingSupport/InstalledScripts=Presets/Scripts") + "/nas"
nas_Installer.replacePath.install=Folder.userData.fullName+"/nas";//%INSTALL%
//nas_Installer.replacePath.startup+"/プリセット/スクリプト/nas";	
